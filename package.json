{
    "name": "eslint-plugin-module-imports",
    "version": "0.0.3",
    "description": "ESLint plugin for ES6 style module imports.",
    "keywords": [
        "eslint",
        "lint",
        "js",
        "javascript",
        "ts",
        "typescript",
        "es6"
    ],
    "license": "ISC",
    "author": "Tao Cumplido",
    "repository": "gitlab:tacugo/eslint-plugin-module-imports",
    "bugs": "https://gitlab.com/tacugo/eslint-plugin-module-imports/issues",
    "files": [
        "README.md",
        "dist/**/*"
    ],
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "test": "cross-env TS_NODE_TRANSPILE_ONLY=true mocha -r ts-node/register src/**/*.spec.ts",
        "lint": "eslint \"**/*.{js,ts}\""
    },
    "dependencies": {
        "builtin-modules": "^3.1.0",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@types/eslint": "^6.1.3",
        "@types/estree": "^0.0.39",
        "@types/node": "^12.12.7",
        "@typescript-eslint/eslint-plugin": "^2.7.0",
        "@typescript-eslint/parser": "^2.7.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.5.0",
        "eslint-plugin-prettier": "^3.1.1",
        "husky": "^3.0.9",
        "lint-staged": "^9.4.3",
        "mocha": "^6.2.2",
        "prettier": "^1.19.1",
        "ts-node": "^8.5.0",
        "typescript": "^3.7.2"
    }
}

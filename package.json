{
	"name": "eslint-plugin-style",
	"version": "0.1.7",
	"description": "ESLint plugin for different stylistic issues",
	"keywords": [
		"eslint",
		"lint",
		"js",
		"javascript",
		"ts",
		"typescript",
		"es6"
	],
	"license": "ISC",
	"author": "Tao Cumplido",
	"repository": "https://github.com/tao-cumplido/eslint-plugin-style",
	"bugs": "https://github.com/tao-cumplido/eslint-plugin-style/issues",
	"files": [
		"README.md",
		"dist/**/*"
	],
	"main": "dist/index.js",
	"volta": {
		"node": "16.13.1"
	},
	"scripts": {
		"prepublishOnly": "npm run test && npm run build",
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"test": "jest",
		"lint": "eslint \"**/*.{js,ts}\"",
		"format": "prettier --write \"**/*.{js,ts,json,md}\""
	},
	"dependencies": {
		"builtin-modules": "3.2.0",
		"tslib": "2.3.1"
	},
	"devDependencies": {
		"@types/eslint": "8.2.1",
		"@types/jest": "27.4.0",
		"@types/node": "16.11.18",
		"@typescript-eslint/eslint-plugin": "5.9.0",
		"@typescript-eslint/parser": "5.9.0",
		"@typescript-eslint/types": "5.9.0",
		"ajv": "8.8.2",
		"eslint": "8.6.0",
		"eslint-plugin-no-only-tests": "2.6.0",
		"eslint-plugin-style": "0.1.7",
		"husky": "4.3.0",
		"jest": "27.4.5",
		"lint-staged": "10.5.2",
		"prettier": "2.5.1",
		"rimraf": "3.0.2",
		"ts-jest": "27.1.2",
		"typescript": "4.5.4"
	},
	"peerDependencies": {
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.0"
	},
	"peerDependenciesMeta": {
		"@typescript-eslint/parser": {
			"optional": true
		}
	}
}
